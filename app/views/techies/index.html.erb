<h1>Listing techies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Lastname</th>
    <th>Position</th>
    <th>Skill</th>
    <th>Bio</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @techies.each do |techy| %>
  <tr>
    <td><%= techy.name %></td>
    <td><%= techy.lastname %></td>
    <td><%= techy.position %></td>
    <td><%= techy.skill %></td>
    <td><%= techy.bio %></td>
    <td><%= link_to 'Show', techy %></td>
    <td><%= link_to 'Edit', edit_techy_path(techy) %></td>
    <td><%= link_to 'Destroy', techy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Techie', new_techy_path %>
